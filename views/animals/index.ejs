<% include ../partials/header %>
  <h1><%= zoo.name%> 's animals</h1>
  <% zoo.animals.forEach(function(animal){ %>
    <p>
      <a href="/animals/<%= animal._id%>">See More about <%= animal.name%></a>

    </p>
    <p>
      <a href="/animals/<%= animal._id%>/edit">Edit <%= animal.name%></a>
    </p>
  <% })%>
  <a href="/zoos/<%= zoo.id%>/animals/new">Add a new animal!</a>

  <p>
    <a href="/">See all zoos</a>
  </p>
<% include ../partials/footer %>